{
	"cli": {
		"requireCommit": true
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_ROUTER_APP_ROOT": "./app"
			}
		},
		"production": {
			"distribution": "store",
			"env": {
				"EXPO_ROUTER_APP_ROOT": "./app"
			},
			"ios": {
				"resourceClass": "m-medium",
				"buildConfiguration": "Release"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
